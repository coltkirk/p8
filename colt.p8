--tab 3 tools
function getframe(ani)
   return ani[flr(t/20)%#ani+1]
end

function drawspr(_spr,_x,_y,_bx,_by,_flip)
   palt(0,false)
   palt(5,true)
   spr(_spr,_x,_y,_bx,_by,_flip)
end

function rectfill2(_x,_y,_w,_h,_c)
   rectfill(_x,_y,_x+max(_w-1,0),_y+max(_h-1,0),_c)
end

--outline 8direction print
function oprint8(_t,_x,_y,_c,_c2)
   for i=1,8 do
      print(_t,_x+dirx[i],_y+diry[i],_c2)
   end
   print(_t,_x,_y,_c)
end

