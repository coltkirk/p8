-->tab 4 gameplay
function moveplayer(dx,dy)
   local destx,desty=p_x+dx,p_y+dy
   local tle=mget(destx,desty)
   
   --animation
   if dy<0 then
      _spr=pu_ani
      p_flip=false
   elseif dy>0 then
      _spr=p_ani
      p_flip=false
   elseif dx>0 then
      _spr=ps_ani
      p_flip=false
   elseif dx<0 then
      _spr=ps_ani
      p_flip=true
   end
   
   if fget(tle,0) then --wall                   
      p_sox,p_soy=dx*8,dy*8
      p_ox,p_oy=0,0
      p_t=0
      _upd=update_pturn
      p_mov=mov_bump
      if fget(tle,1) then
         trig_bump(tle,destx,desty)
      end
   else
      p_x+=dx
      p_y+=dy

      p_sox,p_soy=-dx*8,-dy*8
      p_ox,p_oy=p_sox,p_soy
      p_t=0
      _upd=update_pturn
      p_mov=mov_walk
   end
end

function trig_bump(tle,destx,desty)
   if tle==42 or tle==43 or tle==44 then
      sfx(25)--hotel sign   --mset to swap tile
      if destx==001 and desty==006 or destx==002 and desty==006 or destx==003 and desty==006 then
         showmsg({"  you read the sign","","hOTEL LE nINJA"," aN iSLE oF pINK"," iN AN oCEAN OF gREEN"})
         --elseif for different messages per destx/desty
      end
   elseif tle==99 or tle==100 then --door
      sfx(26)
      fastmsg("hellllllo?",120)
   end
end

