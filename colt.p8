--tab 2 draws
function draw_game()
   cls(3)
   map()
   drawspr(getframe(_spr),p_x*8+p_ox,p_y*8+p_oy,2,2,p_flip)
   for m in all(mob) do
      drawspr(getframe(m.ani),m.x*8,m.y*8,2,2,false)
   end
end

function draw_gameover()
end

