function _init()
   t=0
   p_ani={236,238}
   pu_ani={232,234}
   ps_ani={228,230}

   dirx={-1,1,0,0,1,1,-1,-1}
   diry={0,0,-1,1,-1,1,1,-1}
   music(0)
   _upd=update_game
   _drw=draw_game
   startgame()
end

function _update60()
   t+=1
   _upd()
end

function _draw()
   _drw()
   drawind()
end

function startgame()
   buttbuff=-1

   mob={}
   addmob(0,5,11)
   
   p_x=14
   p_y=13
   p_ox=0
   p_oy=0
   p_sox=0
   p_soy=0
   _spr=p_ani
   p_flip=false
   p_mov=nil
   p_t=0
   wind={}
   talkwind=nil
end

