--tab 3 tools
function getframe(ani)
   return ani[flr(t/25)%#ani+1]
end

function drawspr(_spr,_x,_y,_bx,_by)
   palt(0,false)
   palt(7,true)
   spr(_spr,_x,_y,_bx,_by)
end
