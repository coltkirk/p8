--tab0
function _init()
   t=0
   p_ani={236,238}
--   pu_ani={232,234}
--   ps_ani={228,230}
   dirx={-1,1,0,0}
   diry={0,0,-1,1}
   _upd=update_game
   _drw=draw_game
   startgame()
end

function _update60()
   t+=1
   _upd()
end

function _draw()
   _drw()
end

function startgame()
   p_x=7
   p_y=11
   p_ox=0
   p_oy=0
   p_sox=0
   p_soy=0
   p_t=0
end

-->8
--tab 1 updates
function update_game()
   for i=0,3 do
      if btnp(i) then
         local dx=dirx[i+1]
         local dy=diry[i+1]
         p_x+=dx
         p_y+=dy
         p_sox=-dx*8
         p_soy=-dy*8
         p_ox,p_oy=p_sox,p_soy
         p_t=0
         _upd=update_pturn
         return
      end
   end
end

function update_pturn()
   p_t=min(p_t+0.125,1)
   p_ox=p_sox*(1-p_t)
   p_oy=p_soy*(1-p_t)
   if p_t==1 then
      _upd=update_game
   end
end

function update_gameover()
end

-->8
--tab 2 draws
function draw_game()
   cls(0)
   map()
   drawspr(getframe(p_ani),p_x*8+p_ox,p_y*8+p_oy,2,2)
end

function draw_gameover()
end

